{
  "student_endpoints": {
    "list_students": {
      "endpoint": "GET /students",
      "response_example": [
        {
          "id": "12345",
          "name": "John Doe",
          "department": "Computer Science",
          "year": 2,
          "email": "john.doe@example.com",
          "onboarded_date": "2023-09-01T10:00:00Z",
          "last_active": "2023-10-15T14:30:00Z"
        },
        {
          "id": "67890",
          "name": "Jane Smith",
          "department": "Electrical Engineering",
          "year": 3,
          "email": "jane.smith@example.com",
          "onboarded_date": "2022-08-15T09:30:00Z",
          "last_active": "2023-10-14T16:45:00Z"
        }
      ]
    },
    "get_student": {
      "endpoint": "GET /students/{student_id}",
      "response_example": {
        "id": "12345",
        "name": "John Doe",
        "department": "Computer Science",
        "year": 2,
        "email": "john.doe@example.com",
        "onboarded_date": "2023-09-01T10:00:00Z",
        "last_active": "2023-10-15T14:30:00Z"
      }
    },
    "add_student": {
      "endpoint": "POST /students",
      "request_example": {
        "name": "New Student",
        "department": "Mathematics",
        "year": 1,
        "email": "new.student@example.com"
      },
      "response_example": {
        "id": "54321",
        "name": "New Student",
        "department": "Mathematics",
        "year": 1,
        "email": "new.student@example.com",
        "onboarded_date": "2023-10-16T11:20:00Z",
        "last_active": "2023-10-16T11:20:00Z"
      }
    },
    "update_student": {
      "endpoint": "PUT /students/{student_id}",
      "request_example": {
        "name": "Updated Name",
        "department": "Physics",
        "year": 2,
        "email": "updated.email@example.com"
      },
      "response_example": {
        "id": "12345",
        "name": "Updated Name",
        "department": "Physics",
        "year": 2,
        "email": "updated.email@example.com",
        "onboarded_date": "2023-09-01T10:00:00Z",
        "last_active": "2023-10-16T13:45:00Z"
      }
    },
    "delete_student": {
      "endpoint": "DELETE /students/{student_id}",
      "response_example": {
        "message": "Student with ID 12345 has been deleted successfully"
      }
    }
  },
